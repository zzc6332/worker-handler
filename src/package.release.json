{
  "name": "worker-handler",
  "version": "0.2.10",
  "description": "Worker-handler provides a convenient capability for posting messages between the Main thread and the Worker thread when using Web Worker in javascript or typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzc6332/worker-handler"
  },
  "exports": {
    ".": {
      "types": "./main.d.ts",
      "import": "./main.js",
      "require": "./main.js"
    },
    "./main": {
      "types": "./main.d.ts",
      "import": "./main.js",
      "require": "./main.js"
    },
    "./worker": {
      "types": "./worker.d.ts",
      "import": "./worker.js",
      "require": "./worker.js"
    }
  },
  "keywords": [
    "worker",
    "Web Worker",
    "threads",
    "javascript",
    "typescript",
    "front-end"
  ],
  "author": "zzc6332",
  "license": "MIT"
}
